{
  "name": "keyboard-manager-pro",
  "version": "1.0.0",
  "description": "This package helps manage keyboard shortcuts and sequences for your react application",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "push": "yalc publish --push",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "release": "npm run build && changeset publish",
    "version": "changeset version"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.8",
    "@vitest/expect": "^3.0.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.2",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "yalc": "^1.0.0-pre.53"
  },
  "keywords": [
    "react",
    "keyboard",
    "keyboard manager",
    "keyboard manager pro",
    "shortcuts",
    "nextjs"
  ],
  "author": "nilesh",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/t-trafer/keyboard-manager-pro.git"
  }
}
